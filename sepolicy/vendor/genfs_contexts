genfscon debugfs /wlan0/power_stats    u:object_r:debugfs_power_stats:s0

# Smart Charging
genfscon sysfs /class/power_supply/battery/capacity u:object_r:sysfs_battery_supply:s0

# Touchpanel
genfscon sysfs /kernel/touchpanel      u:object_r:sysfs_touchpanel:s0

# tpd gesture
genfscon proc /tpd_gesture               u:object_r:proc_tpd_gesture:s0

